{
    "manifest_version": 2,
    "name": "REIgnition Fixer",
    "description": "Custom styles to improve the interface for REIgnition",
    "version": "0.5.0",
    "icons": {
        "16": "img/icon_16.png",
        "32": "img/icon_32.png",
        "48": "img/icon_48.png",
        "64": "img/icon_64.png",
        "128": "img/icon_128.png"
    },

    "permissions": [
        "http://*.breilabs.com/*"
    ],

    "content_scripts": [
        {
            "matches": [
                "http://*.breilabs.com/*"
            ],
            "css": [
                "css/main.min.css"
            ],
            "run_at": "document_idle"
        }
    ]
}
